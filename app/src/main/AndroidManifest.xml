<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.travelfake">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Travelfake"
        tools:targetApi="31">
        <activity
            android:name=".SearchActivity"
            android:exported="false" />
        <activity
            android:name=".ShowListExpense"
            android:exported="false"
            android:label="Expense Detail" />
        <activity
            android:name=".AddExpenseActivity"
            android:exported="false"
            android:label="Add Expense" />
        <activity
            android:name=".UpdateActivity"
            android:exported="false"
            android:label="Update Trip" />
        <activity
            android:name=".AddTripActivity"
            android:label="Add Trip" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="M-Expenses">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>